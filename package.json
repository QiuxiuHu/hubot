{
  "name": "hubot",
  "version": "2.16.0",
  "author": "hubot",
  "keywords": [
    "github",
    "hubot",
    "campfire",
    "bot"
  ],
  "description": "A simple helpful robot for your Company",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/github/hubot.git"
  },
  "dependencies": {
    "async": ">=0.1.0 <1.0.0",
    "chalk": "^1.0.0",
    "cline": "^0.8.2",
    "coffee-script": "1.6.3",
    "connect-multiparty": "^1.2.5",
    "express": "3.18.1",
    "log": "1.4.0",
    "optparse": "1.0.4",
    "scoped-http-client": "0.11.0"
  },
  "devDependencies": {
    "generator-hubot": "^0.3.1",
    "hubot-mock-adapter": "^1.0.0",
    "coffee-errors": "0.8.6",
    "mocha": "^2.1.0",
    "mockery": "^1.4.0",
    "sinon-chai": "^2.6.0",
    "yo": "^1.4.7"
  },
  "engines": {
    "node": ">= 0.8.x",
    "npm": ">= 1.1.x"
  },
  "main": "./index",
  "bin": {
    "hubot": "./bin/hubot"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "prestart": "script/dotenv",
    "start": "bin/hubot",
    "pretest": "script/dotenv",
    "test": "mocha --require coffee-errors --compilers coffee:coffee-script",
    "test:smoke": "coffee src/**/*.coffee",
    "compile": "coffee --compile --bare --output lib src",
    "compile:watch": "coffee --compile --bare --output lib --watch src",
    "prerelease": "npm update && npm run test:smoke",
    "release": "npm version",
    "postrelease": "npm publish && git push origin master --tags",
    "package": "mkdir -p hubot && cd hubot && yo hubot --force --no-insight"
  }
}
